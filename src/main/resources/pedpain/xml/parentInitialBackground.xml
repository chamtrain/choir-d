    <Item Order="6" Section="start">
      <Description>
        <![CDATA[
        Child's Ethnicity (choose one that best describes your child)
        ]]>
      </Description>
      <Responses>
        <Response ref="ethnicity" Order="1" Type="select1" required="true">
          <item> <value>1</value> <label>Hispanic/Latino</label> </item>
          <item> <value>2</value> <label>Non-Hispanic/Non-Latino</label> </item>
          <item> <value>9</value> <label>Prefer not to respond</label> </item>
        </Response>
      </Responses>
    </Item>
    <Item Order="7" Section="end">
      <Description>
        <![CDATA[
        Child's Race (check all that apply)
        ]]>
      </Description>
      <Responses>
        <Response ref="race" Order="1" Type="select" required="true">
          <item> <value>1</value> <label>White</label> </item>
          <item> <value>2</value> <label>African-American</label> </item>
          <item> <value>3</value> <label>Asian</label> </item>
          <item> <value>4</value> <label>American Indian or Alaska Native</label> </item>
          <item> <value>5</value> <label>Native Hawaiian or Other Pacific Islander</label> </item>
          <item> <value>9</value> <label>None of the above or Prefer not to respond</label> </item>
        </Response>
        <Response ref="race-other" Order="2" Type="input" required="false">
          <label>and/or</label>
          <format> <datatype>text</datatype> <lines>1</lines> </format>
        </Response>
      </Responses>
    </Item>
    <Item Order="8" Section="start">
      <Description>
        <![CDATA[
        What is the name of your child's school? (or enter NA for no answer)
        ]]>
      </Description>
      <Responses>
        <Response ref="school-name" Order="1" Type="input" required="true">
          <format> <datatype>text</datatype> <lines>1</lines> </format>
        </Response>
      </Responses>
    </Item>
    <Item Order="9" Section="start">
      <Description>
        <![CDATA[
        What grade is your child in?
        ]]>
      </Description>
      <Responses>
        <Response ref="school-grade" Order="1" Type="input" min="0" max="20" required="true">
          <label>Grade</label>
          <format> <datatype>integer</datatype> </format>
        </Response>
      </Responses>
    </Item>
    <Item Order="10">
      <Description>
        <![CDATA[
        Type of school program (check all that apply)
        ]]>
      </Description>
      <Responses>
        <Response ref="school-pgm" Order="1" Type="select" required="true">
          <item> <value>1</value> <label>Public School full-time</label> </item>
          <item> <value>6</value> <label>Public School part-time</label> </item>
          <item> <value>2</value> <label>Private School</label> </item>
          <item> <value>3</value> <label>Independent Study</label> </item>
          <item> <value>4</value> <label>Home/Hospital Programming</label> </item>
          <item> <value>5</value> <label>Not currently enrolled in any schooling</label> </item>
          <item> <value>99</value> <label>None of the above or Prefer not to respond</label> </item>
        </Response>
        <Response ref="school-pgm-other" Order="2" Type="input" required="false">
          <label>and/or</label>
          <format> <datatype>text</datatype> <lines>1</lines> </format>
        </Response>
      </Responses>
    </Item>
    <Item Order="11" Section="end">
      <Description>
        <![CDATA[
        How many days of school has your child missed in the past <b>month</b> due to his/her health? Enter 0 if none.
        ]]>
      </Description>
      <Responses>
        <Response ref="missed-school-days" Order="1" Type="input" min="0" max="31" required="true">
          <label>Days</label>
          <format> <datatype>integer</datatype> </format>
        </Response>
      </Responses>
    </Item>
  </Items>
</Form>