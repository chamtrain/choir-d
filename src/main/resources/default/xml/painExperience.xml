	       <item><label>Punishing</label><value>17</value></item> 
	       <item><label>Cruel</label><value>18</value></item>
	    </Response> 
    </Responses>
  </Item>
  <Item Order="4" ItemResponse="" ItemScore="" RequiredMin="1" RequiredMax="100" required="true" Class="registrySurvey" Align="Horizontal" DescriptionPosition="above" >
    <Description><![CDATA[Please describe the timing of your pain]]></Description>
    <Responses>
        <Response Order="1" Type="select" align="vertical" Class="registrySurveyCBInline" Appearance="full">
	       <ref>PAINTIME</ref>
	       <width>295px</width>
	       <item><label>Brief</label><value>1</value></item> 
	       <item><label>Constant</label><value>2</value></item> 
	       <item><label>Comes and goes</label><value>3</value></item> 
	       <item><label>Continuous</label><value>4</value></item> 
	       <item><label>Always there</label><value>5</value></item> 
	       <item><label>Appears and disappears</label><value>6</value></item> 
	       <item><label>Intermittent</label><value>7</value></item> 
        </Response>
        <Response Order="2" required="false" Type="input"  Class="registrySurvey" Align="Vertical"><Scores />
