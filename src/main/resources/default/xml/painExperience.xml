            <ref>text</ref>
            <label>and/or</label>
            <location>above</location>
            <format>
                <datatype>text</datatype>
		<lines>5</lines>
		<charwidth>80</charwidth>
            </format>
            <hint>Optional: you may enter additional descriptions</hint>
        </Response>
    </Responses>
  </Item>
