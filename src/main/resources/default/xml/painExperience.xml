<Form Description="Pain Experience" DateStarted="" DateFinished="" questionsPerPage="1" Class="surveyInlineBordered">
 <Items class="surveyInline">     
  <Item Order="1" ItemScore="" RequiredMin="1" RequiredMax="3" Align="Horizontal" Class="registrySurvey" >
	<Description><![CDATA[How long have you had your pain problem?]]></Description>
	<Alert>Please enter a valid integer.</Alert>
    <Responses>
      <Response Order="1" required="false" Type="input"   Class="registrySurveyAnswer"  >
        <ref>PAINDURATION</ref>
        <label>Years</label>
        <min>0</min>
        <max>99</max>
        <format>
          <datatype>integer</datatype>
          <lines>1</lines>
          <charwidth>2</charwidth>
        </format>
        <hint>Enter an integer value</hint>
        <Scores />
      </Response>
      <Response Order="2" required="false" Type="input" Class="registrySurveyAnswer"  >
        <ref>PAINDURATION</ref>
        <label>Months</label>
        <min>0</min>
        <max>12</max>
        <location>right</location>
        <format>
          <datatype>integer</datatype>
          <lines>1</lines>
          <charwidth>2</charwidth>
        </format>
        <hint>Enter an integer value</hint>
        <Scores />
      </Response>
      <Response Order="3" required="false" Type="input" Class="registrySurveyAnswer"  >
        <ref>PAINDURATION</ref>
        <label>Days</label>
        <min>0</min>
        <max>31</max>
        <location>right</location>
        <format>
          <datatype>integer</datatype>
          <lines>1</lines>
          <charwidth>2</charwidth>
        </format>
        <hint>Enter an integer value</hint>
        <Scores />
      </Response>
    </Responses>
  </Item>
  <Item Order="2" ItemResponse="" ItemScore="" RequiredMin="1" RequiredMax="100" Class="registrySurvey" Align="Horizontal" DescriptionPosition="above" >
	  <Description><![CDATA[Briefly describe how your pain started]]></Description>
	<Responses>
       <Response Order="1" required="true" Type="input"  Class="surveyAugText" Align="Vertical"><Scores />
		    <label></label>
            <location>above</location>
            <format>
                <datatype>text</datatype>
		<lines>5</lines>
		<charwidth>80</charwidth>
            </format>
		 </Response>
    </Responses>
  </Item>
  <Item Order="3" ItemResponse="" ItemScore="" RequiredMin="1" RequiredMax="100" Class="registrySurvey" Align="Horizontal" DescriptionPosition="above" >
	  <Description><![CDATA[Describe your current pain]]></Description>
	  
    <Responses>
        <Response Order="1" Type="select" align="horizontal" Class="registrySurveyCBInline" Appearance="full">
		   <ref>PAINQUALITY</ref>
           <width>295px</width>
	       <item><label>Throbbing</label><value>1</value></item> 
	       <item><label>Shooting</label><value>2</value></item> 
	       <item><label>Stabbing</label><value>3</value></item> 
	       <item><label>Sharp</label><value>4</value></item> 
	       <item><label>Cramping</label><value>5</value></item> 
	       <item><label>Gnawing</label><value>6</value></item> 
	       <item><label>Hot</label><value>7</value></item> 
	       <item><label>Burning</label><value>8</value></item> 
	       <item><label>Aching</label><value>9</value></item> 
		</Response> 
		<Response Order="2" Type="select" align="horizontal" Class="registrySurveyCBInline" Appearance="full">
		   <ref>PAINQUALITY</ref>
           <item><label>Heavy</label><value>10</value></item> 
	       <item><label>Tender</label><value>11</value></item> 
	       <item><label>Splitting</label><value>12</value></item> 
	       <item><label>Tiring</label><value>13</value></item> 
	       <item><label>Exhausting</label><value>14</value></item> 
	       <item><label>Sickening</label><value>15</value></item> 
